{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch Current File",
            "script": "${file}",
            "args": [
                "-NoProfile",
                "-ExecutionPolicy",
                "Bypass"
            ],
            "cwd": "${file}"
        }
    ]
}